  "scripts": {
    "preinstall": "npm link mocha should browserify uglify-js",
    "start":"python -m SimpleHTTPServer 3000",
    "test": "mocha tests/main.js",
    "build:dev": "browserify -t [ babelify --loose all ] -d -e ./bbone.js -s bbone -o ./dist/bbone.js",
    "build:prod": "browserify -t [ babelify --loose all ] ./bbone.js -s bbone | uglifyjs -o ./dist/bbone.js",
    "build:bbone": "browserify -t [ babelify --loose all ] ./bbone.js -s bbone | uglifyjs -o ./dist/bbone.js  -b indent-level=2",
    "build": "npm run build:dev && npm run test"
  },
  "dependencies": {},
  "devDependencies": {
    "browserify": "latest",
    "babelify": "latest",
    "mocha": "latest",
    "should": "latest",
    "uglify-js": "latest"
  }
